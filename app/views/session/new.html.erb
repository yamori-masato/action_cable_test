<h1>Session#new</h1>

<%= form_with(scope: :session, local: true) do |form| %>

<div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
</div>

<div class="field">
    <%= form.label :password %>
    <%= form.text_field :password %>
</div>

<div class="actions">
    <%= form.submit %>
</div>
<% end %>